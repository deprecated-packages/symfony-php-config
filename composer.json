{
    "name": "symplify/symfony-php-config",
    "description": "Tools that easy work with Symfony PHP Configs",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "symfony/dependency-injection": "^5.2",
        "symplify/symplify-kernel": "^9.3.8",
        "symplify/package-builder": "^9.3.8"
    },
    "require-dev": {
        "symfony/http-kernel": "^4.4|^5.2",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "0.12.86"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyPhpConfig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyPhpConfig\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
