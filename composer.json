{
    "name": "symplify/symfony-php-config",
    "description": "Tools that easy work with Symfony PHP Configs",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/dependency-injection": "^5.3",
        "symplify/symplify-kernel": "^9.4.15",
        "symplify/package-builder": "^9.4.15"
    },
    "require-dev": {
        "symfony/http-kernel": "^5.3",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.91"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\SymfonyPhpConfig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\SymfonyPhpConfig\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
